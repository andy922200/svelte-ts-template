{
  "name": "svelete-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint-check": "npx eslint src --ext .js,.ts,tsx,svelte",
    "lint-fix": "npx eslint src --ext .js,.ts,tsx,svelte --fix",
    "css-check": "npx stylelint \"src/**/*.{css,scss,svelte}\" ",
    "css-fix": "npx stylelint \"src/**/*.{css,scss,svelte}\" --fix",
    "check": "svelte-check --tsconfig ./tsconfig.json"
